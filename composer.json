{
  "name": "pinguo-guzhongzhi/config-center-sdk",
  "description": "config center php sdk",
  "type": "library",
  "authors": [
    {
      "name": "guzhongzhi",
      "email": "guzhongzhi@camera360.com"
    }
  ],
  "repositories": {
    "lib-compatible": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/lib-compatible"
    }
  },
  "require": {
    "PGWireless/lib-compatible": "dev-ali-master",
    "symfony/console": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "PGConfig\\": "src/"
    }
  },
  "bin": [
    "bin/pg-config-dog"
  ],
  "replace": {
    "bower-asset/jquery": "*",
    "bower-asset/inputmask": "*",
    "bower-asset/jquery.inputmask": "*",
    "bower-asset/punycode": "*",
    "bower-asset/yii2-pjax": "*"
  },
  "conflict": {
    "bower-asset/jquery": "*",
    "bower-asset/inputmask": "*",
    "bower-asset/jquery.inputmask": "*",
    "bower-asset/punycode": "*",
    "bower-asset/yii2-pjax": "*"
  }
}
